<program xmlns="http://ci.uchicago.edu/swift/2009/02/swiftscript"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <variable name="a" type="string" isGlobal="false" xsi:nil="true"/>
  <assign src="line 3">
   <variableReference>a</variableReference>
   <stringConstant>hello</stringConstant>
  </assign>
  <call proc="trace" src="line 3">
    <input><variableReference>a</variableReference></input>
  </call>
</program>
